// Define the options on the spin wheel
var options = ["Option 1", "Option 2", "Option 3", "Option 4", "Option 5"];

function spinWheel() {
    // Spin the wheel and select a random option
    var selectedOption = options[Math.floor(Math.random() * options.length)];
    document.getElementById("result").textContent = "You spun the wheel and got: " + selectedOption;
}
